<display-name><![CDATA[Scriptable task]]></display-name>
<script encoded='false'><![CDATA[var session = vPortObj.session;

dHCPOptionObj = new VSPKDHCPOption();
if (externalID) {
   dHCPOptionObj.externalID = externalID;
}
if (length) {
   dHCPOptionObj.length = length;
}
if (type) {
   dHCPOptionObj.type = type;
}
if (value) {
   dHCPOptionObj.value = value;
}

vPortObj.createDHCPOption(session, dHCPOptionObj);
]]></script>
