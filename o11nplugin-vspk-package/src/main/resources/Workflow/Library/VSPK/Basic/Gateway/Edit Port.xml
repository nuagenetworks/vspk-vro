<display-name><![CDATA[Scriptable task]]></display-name>
<script encoded='false'><![CDATA[var session = portObj.session;

if (VLANRange) {
   portObj.VLANRange = VLANRange;
} else if (portObj.VLANRange) {
   portObj.VLANRange = "";
}
if (description) {
   portObj.description = description;
} else if (portObj.description) {
   portObj.description = "";
}
if (externalID) {
   portObj.externalID = externalID;
} else if (portObj.externalID) {
   portObj.externalID = "";
}
if (name) {
   portObj.name = name;
} else if (portObj.name) {
   portObj.name = "";
}
if (physicalName) {
   portObj.physicalName = physicalName;
} else if (portObj.physicalName) {
   portObj.physicalName = "";
}
portObj.portType = portType;
portObj.save(session);]]></script>
