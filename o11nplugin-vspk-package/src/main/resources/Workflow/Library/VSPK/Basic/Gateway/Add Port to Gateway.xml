<display-name><![CDATA[Scriptable task]]></display-name>
<script encoded='false'><![CDATA[var session = gatewayObj.session;

portObj = new VSPKPort();
if (VLANRange) {
   portObj.VLANRange = VLANRange;
}
if (description) {
   portObj.description = description;
}
if (externalID) {
   portObj.externalID = externalID;
}
if (name) {
   portObj.name = name;
}
if (physicalName) {
   portObj.physicalName = physicalName;
}
portObj.portType = portType;

gatewayObj.createPort(session, portObj);
]]></script>
