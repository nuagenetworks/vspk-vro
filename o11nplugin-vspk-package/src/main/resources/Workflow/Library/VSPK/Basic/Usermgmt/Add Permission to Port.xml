<display-name><![CDATA[Scriptable task]]></display-name>
<script encoded='false'><![CDATA[var session = portObj.session;

permissionObj = new VSPKPermission();
if (externalID) {
   permissionObj.externalID = externalID;
}
if (name) {
   permissionObj.name = name;
}
permissionObj.permittedAction = permittedAction;
if (permittedEntityID) {
   permissionObj.permittedEntityID = permittedEntityID;
}

portObj.createPermission(session, permissionObj);
]]></script>
