} else if (qOSObj.burst) {
   qOSObj.burst = "";
}
if (description) {
   qOSObj.description = description;
} else if (qOSObj.description) {
   qOSObj.description = "";
}
if (externalID) {
   qOSObj.externalID = externalID;
} else if (qOSObj.externalID) {
   qOSObj.externalID = "";
}
if (name) {
   qOSObj.name = name;
} else if (qOSObj.name) {
   qOSObj.name = "";
}
if (peak) {
   qOSObj.peak = peak;
} else if (qOSObj.peak) {
   qOSObj.peak = "";
}
qOSObj.rateLimitingActive = rateLimitingActive;
qOSObj.serviceClass = serviceClass;
qOSObj.save(session);]]></script>
