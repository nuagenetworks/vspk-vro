<workflow-item name='item1' out-name='item0' type='task' >
<display-name><![CDATA[Scriptable task]]></display-name>
<script encoded='false'><![CDATA[var session = aggregatedDomainObj.session;

sPATSourcesPoolObj = new VSPKSPATSourcesPool();
sPATSourcesPoolObj.addressList = addressList;
if (externalID) {
   sPATSourcesPoolObj.externalID = externalID;
}
if (name) {
   sPATSourcesPoolObj.name = name;
}

aggregatedDomainObj.createSPATSourcesPool(session, sPATSourcesPoolObj);
]]></script>
