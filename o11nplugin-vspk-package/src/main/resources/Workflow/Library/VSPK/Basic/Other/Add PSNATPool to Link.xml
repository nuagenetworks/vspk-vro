<script encoded='false'><![CDATA[var session = linkObj.session;

pSNATPoolObj = new VSPKPSNATPool();
if (description) {
   pSNATPoolObj.description = description;
}
if (endAddress) {
   pSNATPoolObj.endAddress = endAddress;
}
