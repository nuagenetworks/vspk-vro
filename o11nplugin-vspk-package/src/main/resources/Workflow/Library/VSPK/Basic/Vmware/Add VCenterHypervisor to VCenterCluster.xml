}
if (name) {
   vCenterHypervisorObj.name = name;
}
if (vmNetworkPortgroup) {
   vCenterHypervisorObj.vmNetworkPortgroup = vmNetworkPortgroup;
}

vCenterClusterObj.createVCenterHypervisor(session, vCenterHypervisorObj);
